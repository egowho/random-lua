-- Create the dropdown for rebirth options
local RebirthDropdown = MainTab:CreateDropdown({
    Name = "Rebirth Option",
    Description = "Choose which rebirth to use",
    Options = rebirthOptions,
    CurrentOption = rebirthOptions[1],
    Callback = function(Selected) end
}, "RebirthDropdown")

-- Create the auto-rebirth toggle
local RebirthToggle = MainTab:CreateToggle({
    Name = "Auto Rebirth",
    Description = "Automatically rebirth based on the selected option",
    CurrentValue = false,
    Callback = function(Value)
        if Value then
            _G.AutoRebirthConnection = game:GetService("RunService").Heartbeat:Connect(function()
                task.wait()
                local selectedRebirth = RebirthDropdown.CurrentOption
                local rebirthNumber = tonumber(selectedRebirth:match("%d+%.?%d*")) or 1
                task.wait()
                if selectedRebirth:find("k") then
                    rebirthNumber = rebirthNumber * 1e3 -- Convert k to 1,000
                elseif selectedRebirth:find("M") then
                    rebirthNumber = rebirthNumber * 1e6 -- Convert M to 1,000,000
                elseif selectedRebirth:find("B") then
                    rebirthNumber = rebirthNumber * 1e9 -- Convert B to 1,000,000,000
                elseif selectedRebirth:find("T") then
                    rebirthNumber = rebirthNumber * 1e12 -- Convert T to 1,000,000,000,000
                elseif selectedRebirth:find("Qa") then
                    rebirthNumber = rebirthNumber * 1e15 -- Convert Qa to 1,000,000,000,000,000
                elseif selectedRebirth:find("Qi") then
                    rebirthNumber = rebirthNumber * 1e18 -- Convert Qi to 1,000,000,000,000,000,000
                elseif selectedRebirth:find("Sx") then
                    rebirthNumber = rebirthNumber * 1e21 -- Convert Sx to 1,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Sp") then
                    rebirthNumber = rebirthNumber * 1e24 -- Convert Sp to 1,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Oc") then
                    rebirthNumber = rebirthNumber * 1e27 -- Convert Oc to 1,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("No") then
                    rebirthNumber = rebirthNumber * 1e30 -- Convert No to 1,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Dc") then
                    rebirthNumber = rebirthNumber * 1e33 -- Convert Dc to 1,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Ud") then
                    rebirthNumber = rebirthNumber * 1e36 -- Convert Ud to 1,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Dd") then
                    rebirthNumber = rebirthNumber * 1e39 -- Convert Dd to 1,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Td") then
                    rebirthNumber = rebirthNumber * 1e42 -- Convert Td to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Z") then
                    rebirthNumber = rebirthNumber * 1e45 -- Convert Z to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("Y") then
                    rebirthNumber = rebirthNumber * 1e48 -- Convert Y to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("X") then
                    rebirthNumber = rebirthNumber * 1e51 -- Convert X to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("W") then
                    rebirthNumber = rebirthNumber * 1e54 -- Convert W to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("V") then
                    rebirthNumber = rebirthNumber * 1e57 -- Convert V to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("U") then
                    rebirthNumber = rebirthNumber * 1e60 -- Convert U to 1,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000,000
                elseif selectedRebirth:find("∞") then
                    rebirthNumber = math.huge -- Set to infinity
                end
                task.wait(0.1)
                local args = { [1] = rebirthNumber }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Rebirth"):FireServer(unpack(args))
            end)
        else
            if _G.AutoRebirthConnection then
                _G.AutoRebirthConnection:Disconnect()
            end
        end
    end
}, "AutoRebirthToggle")




--// Egg Tab \\--


local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local mainGui = playerGui:WaitForChild("Main")

local leftHolder = mainGui:WaitForChild("LeftHolder")
local rightHolder = mainGui:WaitForChild("RightHolder")
local bottomHolder = mainGui:WaitForChild("BottomHolder")

local desiredPositions = {
    Left = UDim2.new(0, 0, 0.5, 0),
    Right = UDim2.new(1, 0, 0.5, 0),
    Bottom = UDim2.new(0.5, 0, 1, 0)
}

local function enforcePosition(element, desiredPosition)
    element:GetPropertyChangedSignal("Position"):Connect(function()
        if element.Position ~= desiredPosition then
            element.Position = desiredPosition
        end
    end)
end

enforcePosition(leftHolder, desiredPositions.Left)
enforcePosition(rightHolder, desiredPositions.Right)
enforcePosition(bottomHolder, desiredPositions.Bottom)


